<div class="selectlist">
    <div class="selectlist-backdrop"></div>
    <div class="selectlist-content">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class="pull-right selectlist-zapri">
                    <i class="fa fa-times"></i>
                </div>
                <h4 class="selectlist-naslov">{{title}}</h4>
            </div>
            <div class="panel-body selectlist-body">
                <div class="row">
                    <div class="coll-xs-12">
                        <div class="selectlist-filter"></div>
                    </div>
                </div>
                <div class="selectlist-seznama">
                    <div class="row">
                        <div class="col-xs-5">
                            <div class="selectlist-seznam-levi"></div>
                        </div>
                        <div class="col-xs-2">
                            <div class="selectlist-gumbi">
                                <div class="row">
                                    <div class="col-sm-12">
                                        <a class="btn btn-default selectlist-gumb vsiDesno" title="{{t "std.premakniVseDesno"}}">>></a>
                                    </div>
                                    <div class="col-sm-12">
                                        <a class="btn btn-default selectlist-gumb izbraniDesno" title="{{t "std.premakniIzbraneDesno"}}">></a>
                                    </div>
                                    <div class="col-sm-12">
                                        <a class="btn btn-default selectlist-gumb izbraniLevo" title="{{t "std.premakniIzbraneLevo"}}"><</a>
                                    </div>
                                    <div class="col-sm-12">
                                        <a class="btn btn-default selectlist-gumb vsiLevo" title="{{t "std.premakniVseLevo"}}"><<</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-5">
                            <div class="selectlist-seznam-desni"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>