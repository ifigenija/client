<div class="panel panel-default sodelujoci-uprizoritev">
    <div class="panel-heading" role="tab" id="heading-{{id}}">
        <h4 class="panel-title">
            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#{{id}}" aria-expanded="true" aria-controls="{{id}}">
                {{label}}
            </a>
        </h4>
    </div>
    <div id="{{id}}" class="panel-collapse collapsed" role="tabpanel" aria-labelledby="{{id}}">
        <div class="panel-body">
            <div class="sodelujoci-funkcije"></div>
        </div>
    </div>
</div>